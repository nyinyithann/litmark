{
  "name": "lit-markdown",
  "version": "1.1.0",
  "description": "A package for rendering Markdown in LitElements.",
  "main": "lib/import",
  "type": "module",
  "exports": {
    "types": "./lib/import/index.d.ts",
    "import": "./lib/import/index.js",
    "require": "./lib/commonjs/index.js"
  },
  "types": "lib/import",
  "author": "Austin John Mayer",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/tronicboy1/lit-markdown"
  },
  "bugs": {
    "url": "https://github.com/tronicboy1/lit-markdown/issues"
  },
  "keywords": [
    "marked",
    "markedjs",
    "lit",
    "directive",
    "markdown",
    "markup",
    "html",
    "sanitize"
  ],
  "dependencies": {
    "lit": "^2.3.1",
    "marked": "^4.1.0",
    "sanitize-html": "^2.7.2"
  },
  "devDependencies": {
    "@types/marked": "^4.0.7",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "format": "prettier --write ./",
    "lint": "eslint ./src",
    "build": "tsc && tsc -p tsconfig.node.json"
  }
}
